<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="HorzDivider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>
    <style name="VertDivider">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/md_cyan_600</item>
        <item name="colorPrimaryDark">@color/md_cyan_900</item>
        <item name="colorAccent">@color/md_cyan_300</item>
    </style>

    <style name="AppTheme.Launcher">
        <item name="android:windowBackground">@drawable/launch_screen</item>
        <!-- Optional, on Android 5+ you can modify the colorPrimaryDark color to match the windowBackground color for further branding-->
        <!-- <item name="colorPrimaryDark">@android:color/white</item> -->
    </style>

    <style name="AppTheme.ActionBar">
        <item name="android:background">@color/md_cyan_600</item>
        <item name="android:titleTextColor">@color/md_white_1000</item>
        <item name="android:textColorPrimary">@color/md_white_1000</item>
    </style>
</resources>
